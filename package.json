{
  "name": "portfolio",
  "version": "8.8.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "major": "npm version major",
    "minor": "npm version minor",
    "patch": "npm version patch",
    "preversion": "npm test",
    "version": "npm run lint && npm run build && git add docs",
    "postversion": "git push && git push --tags",
    "build": "ng build --base-href https://eskinderg.github.io/ --configuration production && npm run lang && cd docs && rm -rf * && cd .. && cp -ra dist/browser/. docs",
    "build-local": "ng build && npm run lang",
    "lang": "gulp minify",
    "watch": "ng build --watch --configuration development",
    "test": "ng test --watch=false --code-coverage --no-progress",
    "lint": "ng lint",
    "prepare": "husky",
    "serve:ssr:portfolio": "node dist/server/server.mjs"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^20.1.4",
    "@angular/compiler": "^20.1.4",
    "@angular/core": "^20.1.4",
    "@angular/platform-browser": "^20.1.4",
    "@angular/platform-browser-dynamic": "^20.1.4",
    "@angular/platform-server": "^20.1.4",
    "@angular/router": "^20.1.4",
    "@angular/ssr": "^20.1.4",
    "express": "^5.1.0",
    "gulp": "^5.0.0",
    "gulp-jsonminify": "^1.1.0",
    "gulp-rename": "^2.1.0",
    "rxjs": "~7.8.2",
    "tslib": "^2.8.1",
    "typescript-language-server": "^4.4.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.2.0",
    "@angular-eslint/builder": "20.2.0",
    "@angular-eslint/eslint-plugin": "20.2.0",
    "@angular-eslint/eslint-plugin-template": "^20.2.0",
    "@angular-eslint/schematics": "^20.2.0",
    "@angular-eslint/template-parser": "^20.1.1",
    "@angular/animations": "^20.1.4",
    "@angular/build": "^20.2.0",
    "@angular/cli": "^20.3.0",
    "@angular/compiler-cli": "^20.1.4",
    "@angular/language-server": "^20.2.0",
    "@angular/language-service": "^20.3.0",
    "@types/express": "^5.0.1",
    "@types/jasmine": "~5.1.8",
    "@types/node": "^24.3.0",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-change-detection-strategy": "^0.1.5",
    "eslint-plugin-prettier": "^5.5.0",
    "eslint-plugin-storybook": "^9.1.0",
    "geezify-js": "^1.0.1",
    "husky": "^9.1.7",
    "jasmine-core": "~5.10.0",
    "karma": "~6.4.4",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "prettier": "~3.6.0",
    "prettier-eslint": "^16.4.2",
    "typescript": "~5.8.3"
  }
}
