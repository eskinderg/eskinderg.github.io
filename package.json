{
  "name": "portfolio",
  "version": "5.3.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "major": "npm version major && git push",
    "minor": "npm version minor && git push",
    "patch": "npm version patch && git push",
    "preversion": "npm test",
    "version": "npm run lint && npm run build && git add docs",
    "postversion": "git push && git push --tags",
    "build": "ng build --base-href https://eskinderg.github.io/ && npm run lang",
    "lang": "gulp minify",
    "watch": "ng build --watch --configuration development",
    "test": "ng test --browsers=ChromeHeadless --watch=false --code-coverage",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.0.1",
    "@angular/cdk": "^17.0.0",
    "@angular/common": "^17.0.1",
    "@angular/compiler": "^17.0.1",
    "@angular/core": "^17.0.1",
    "@angular/forms": "^17.0.1",
    "@angular/language-service": "^17.0.1",
    "@angular/material": "^17.0.0",
    "@angular/platform-browser": "^17.0.1",
    "@angular/platform-browser-dynamic": "^17.0.1",
    "@angular/router": "^17.0.1",
    "gulp": "^4.0.2",
    "gulp-jsonminify": "^1.1.0",
    "gulp-rename": "^2.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.5.0",
    "zone.js": "~0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.0.0",
    "@angular-eslint/builder": "17.1.0",
    "@angular-eslint/eslint-plugin": "17.1.0",
    "@angular-eslint/eslint-plugin-template": "17.1.0",
    "@angular-eslint/schematics": "^17.1.0",
    "@angular-eslint/template-parser": "^17.1.0",
    "@angular/cli": "^17.0.0",
    "@angular/compiler-cli": "^17.0.1",
    "@types/jasmine": "~5.1.2",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.18.0",
    "eslint": "^8.56.0",
    "jasmine-core": "~5.1.1",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "tailwindcss": "^3.4.0",
    "typescript": "~5.2.2"
  }
}
