{
  "name": "portfolio",
  "version": "6.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "major": "npm version major && git push",
    "minor": "npm version minor && git push",
    "patch": "npm version patch && git push",
    "preversion": "npm test",
    "version": "npm run lint && npm run build && git add docs",
    "postversion": "git push && git push --tags",
    "build": "ng build --base-href https://eskinderg.github.io/ && npm run lang",
    "lang": "gulp minify",
    "watch": "ng build --watch --configuration development",
    "test": "ng test --browsers=ChromeHeadless --watch=false --code-coverage",
    "lint": "ng lint",
    "prepare": "husky install"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.0.1",
    "@angular/cdk": "^18.0.2",
    "@angular/common": "^18.0.1",
    "@angular/compiler": "^18.0.1",
    "@angular/core": "^18.0.1",
    "@angular/forms": "^18.0.1",
    "@angular/language-service": "^18.0.1",
    "@angular/material": "^18.0.2",
    "@angular/platform-browser": "^18.0.1",
    "@angular/platform-browser-dynamic": "^18.0.1",
    "@angular/router": "^18.0.1",
    "gulp": "^5.0.0",
    "gulp-jsonminify": "^1.1.0",
    "gulp-rename": "^2.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.5.0",
    "zone.js": "~0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.0.1",
    "@angular-eslint/builder": "18.0.1",
    "@angular-eslint/eslint-plugin": "18.0.1",
    "@angular-eslint/eslint-plugin-template": "18.0.1",
    "@angular-eslint/schematics": "^18.0.1",
    "@angular-eslint/template-parser": "^18.0.1",
    "@angular/cli": "^18.0.1",
    "@angular/compiler-cli": "^18.0.1",
    "@types/jasmine": "~5.1.2",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@typescript-eslint/parser": "^7.11.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-storybook": "^0.8.0",
    "husky": "^8.0.0",
    "jasmine-core": "~5.1.1",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "lint-staged": "^15.2.5",
    "prettier": "~3.3.1",
    "prettier-eslint": "^16.3.0",
    "tailwindcss": "^3.4.0",
    "typescript": "~5.4.5"
  }
}
