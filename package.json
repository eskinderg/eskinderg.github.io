{
  "name": "portfolio",
  "version": "8.8.5",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "major": "npm version major",
    "minor": "npm version minor",
    "patch": "npm version patch",
    "preversion": "npm test",
    "version": "npm run lint && npm run build && git add docs",
    "postversion": "git push && git push --tags",
    "build": "ng build --base-href https://eskinderg.github.io/ --configuration production && npm run lang && cd docs && rm -rf * && cd .. && cp -ra dist/browser/. docs",
    "build-local": "ng build && npm run lang",
    "lang": "gulp minify",
    "watch": "ng build --watch --configuration development",
    "test": "ng test --watch=false --code-coverage --no-progress",
    "lint": "ng lint",
    "lint:css": "stylelint \"src/**/*.{css,scss}\"",
    "lint:css:fix": "stylelint \"src/**/*.{css,scss}\" --fix",
    "prepare": "husky",
    "serve:ssr:portfolio": "node dist/server/server.mjs"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^21.0.1",
    "@angular/compiler": "^21.0.1",
    "@angular/core": "^21.0.1",
    "@angular/platform-browser": "^21.0.1",
    "@angular/platform-browser-dynamic": "^21.0.1",
    "@angular/platform-server": "^21.0.1",
    "@angular/router": "^21.0.1",
    "@angular/ssr": "^21.0.1",
    "express": "^5.2.1",
    "gulp": "^5.0.0",
    "gulp-jsonminify": "^1.1.0",
    "gulp-rename": "^2.1.0",
    "rxjs": "~7.8.2",
    "tslib": "^2.8.1",
    "typescript-language-server": "^5.1.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^21.2.0",
    "@angular-eslint/builder": "21.2.0",
    "@angular-eslint/eslint-plugin": "21.2.0",
    "@angular-eslint/eslint-plugin-template": "^21.2.0",
    "@angular-eslint/schematics": "^21.2.0",
    "@angular-eslint/template-parser": "^21.0.1",
    "@angular/animations": "^21.0.1",
    "@angular/build": "^21.2.0",
    "@angular/cli": "^21.2.0",
    "@angular/compiler-cli": "^21.2.0",
    "@angular/language-server": "^21.2.0",
    "@angular/language-service": "^21.1.0",
    "@types/express": "^5.0.1",
    "@types/jasmine": "~6.0.0",
    "@types/node": "^25.3.0",
    "@typescript-eslint/eslint-plugin": "^8.56.0",
    "@typescript-eslint/parser": "^8.55.0",
    "eslint": "^9.39.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-change-detection-strategy": "^0.1.5",
    "eslint-plugin-prettier": "^5.5.0",
    "eslint-plugin-storybook": "^10.2.0",
    "ethiopian-calendar-new": "^1.1.0",
    "geezify-js": "^1.0.1",
    "husky": "^9.1.7",
    "jasmine-core": "~6.1.0",
    "karma": "~6.4.4",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.2.0",
    "prettier": "~3.8.0",
    "prettier-eslint": "^16.4.2",
    "stylelint": "^16.25.0",
    "stylelint-config-standard-scss": "^16.0.0",
    "stylelint-scss": "^7.0.0",
    "typescript": "~5.9.2"
  }
}
